{
  "name": "devcrm-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js -d src/config/data-source.ts",
    "migrate:generate": "npx typeorm-ts-node-esm migration:generate -d src/config/data-source.ts",
    "migrate:run": "npx typeorm-ts-node-esm migration:run -d src/config/data-source.ts",
    "migrate:revert": "npx typeorm-ts-node-esm migration:revert -d src/config/data-source.ts",
    "workers": "tsx src/worker-main.ts"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.58.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "pg": "^8.12.0",
    "puppeteer": "^24.19.0",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.0.1",
    "stripe": "^18.5.0",
    "type-fest": "^4.25.0",
    "typeorm": "^0.3.20",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.15.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.4.5"
  }
}
